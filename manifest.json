{
   "author": "Ankit Sharma",
   "background": {
      "page": "/background/html/background.html"
   },
   "browser_action": {
      "default_icon": "/global/images/icons/128.png",
      "default_popup": "/popup/html/popup.html",
      "default_title": "OneAdmin Extension"
   },
   "content_scripts": [ {
      "js": [ "/background/js/hideseen.js" ],
      "matches": [ "https://*.pfms.nic.in/*", "*://*.pfms.nic.in/*" ]
   } ],
   "description": "OneAdmin extension to handle two-way data in between OneAdmin & PFMS",
   "icons": {
      "128": "/global/images/icons/128.png",
      "16": "/global/images/icons/16.png",
      "32": "/global/images/icons/32.png",
      "48": "/global/images/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "OneAdmin Extension",
   "permissions": [ "storage", "*://*/*", "*://*.appspot.com/*", "*://*.pfms.nic.in/*", "notifications", "tabs", "clipboardWrite" ],
   "short_name": "OAE",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "content_old/*", "content_new/*", "images/*", "global/css/*" ]
}
